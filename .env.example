# Backend & smoke test environment (example)
# Copy to `.env` and fill in real values locally. Do NOT commit `.env`.

# Backend server
PORT=8080
CORS_ORIGIN=http://localhost:5173

# LLM provider
groq_API_BASE=https://api.groq.com/openai/v1
# groq_API_KEY=your_groq_api_key_here

groq_MODEL=openai/gpt-oss-120b

# Jira (used by Node smoke test)
JIRA_BASE_URL=https://your-domain.atlassian.net
# basic | bearer
JIRA_AUTH_TYPE=basic
JIRA_EMAIL=you@example.com
# JIRA_API_TOKEN=your_jira_api_token_here
# JIRA_BEARER=your_bearer_token_if_using_bearer

# For smoke test convenience
JIRA_ISSUE_KEY=PROJ-123

# Optional: if you store AC in a custom field
# JIRA_ACCEPTANCE_FIELD=customfield_12345
